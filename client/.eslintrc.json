{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [

    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 4],
        "max-len": ["error", { "code": 120 }],
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
